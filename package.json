{
  "name": "ponyracer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run --coverage",
    "test:e2e": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "test:e2e:ci": "vite build && start-server-and-test preview http://localhost:4173 'cypress run --e2e --browser chrome'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "type-check:e2e": "tsc --noEmit -p cypress/e2e/tsconfig.json",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "vue-ninja": "vue-ninja"
  },
  "dependencies": {
    "@vee-validate/i18n": "4.9.3",
    "@vee-validate/rules": "4.9.3",
    "axios": "1.4.0",
    "bootstrap": "5.2.3",
    "date-fns": "2.30.0",
    "font-awesome": "4.7.0",
    "vee-validate": "4.9.3",
    "vue": "3.3.2",
    "vue-router": "4.2.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.2.0",
    "@tsconfig/node18": "2.0.1",
    "@types/jsdom": "21.1.1",
    "@types/node": "18.16.8",
    "@vitejs/plugin-vue": "4.2.3",
    "@vitest/coverage-istanbul": "0.30.1",
    "@vue/eslint-config-prettier": "7.1.0",
    "@vue/eslint-config-typescript": "11.0.3",
    "@vue/test-utils": "2.3.2",
    "@vue/tsconfig": "0.4.0",
    "cypress": "^12.1.0",
    "eslint": "8.40.0",
    "eslint-plugin-cypress": "2.13.3",
    "eslint-plugin-vue": "9.12.0",
    "jsdom": "22.0.0",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "4.3.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "start-server-and-test": "2.0.0",
    "typescript": "5.0.4",
    "vite": "4.3.5",
    "vitest": "0.30.1",
    "vue-ninja": "0.5.3",
    "vue-router-mock": "0.2.0",
    "vue-tsc": "1.6.4"
  }
}
